<div class="page-header">
  <%= link_to snapshots_path, class: 'btn btn-default' do %>
    <span class="glyphicon glyphicon-list-alt"></span>
    Back
  <% end %>
  <%= link_to edit_snapshot_path(@snapshot), class: 'btn btn-primary' do %>
    <span class="glyphicon glyphicon-pencil"></span>
    Edit
  <% end %>
  <h1>Show snapshot</h1>
</div>

<dl class="dl-horizontal">
  <dt>Name:</dt>
  <dd><%= @snapshot.name %></dd>

  <dt>Description:</dt>
  <dd><%= @snapshot.description %></dd>

  <dt>Video:</dt>
  <dd><%= @snapshot.video %></dd>

  <dt>File:</dt>
  <dd><%= @snapshot.file %></dd>

</dl>
